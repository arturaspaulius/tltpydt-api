{
  "basePath": "/",
  "entryFile": "./api/genroot.ts",
  "spec": {
    "outputDirectory": "./api/_gen/swagger",
    "host": "localhost:3000",
    "securityDefinitions": {
      "api_key": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      },
      "basic_auth_internal": {
        "type": "basic"
      }
    }
  },
  "routes": {
    "routesDir": "./api/_gen/routes",
    "iocModule": "./lib/ioc",
    "authenticationModule": "./lib/auth/expressAuthentication"
  },
  "ignore": [
    "**/node_modules/pydt-shared/lib/_gen/**",
    "**/node_modules/@types/lodash/**"
  ]
}